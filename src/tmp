typedef struct{
		int integer;
		const char** strings;
}EnumValueStr_t;

typedef struct{
	union{
		float floatval;
		int integer;
		const char* string;
		byte boolean;
		vec4_t vec4;
		vec3_t vec3;
		vec2_t vec2;
		ucolor_t color;
		EnumValueStr_t enumval; /* For Cvar_Register and Cvar_ValidateNewVar only */
	};
}CvarValue;


typedef struct{
	union{
		int imin;
		float fmin;
	};
	union{
		int imax;
		float fmax;
	};
}CvarLimits;
